{
    "compilerOptions": {
        "rootDir": "src",
        "outDir": "dist",
        "target": "ESNext",
        "module": "ESNext",
        "declaration": true,
        "declarationMap": true,
        "emitDeclarationOnly": false,
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    },
    "include": ["src/**/*.ts"],
  }