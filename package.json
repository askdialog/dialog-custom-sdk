{
  "name": "@askdialog/dialog-sdk",
  "version": "1.0.14",
  "private": false,
  "description": "Dialog SDK",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "clean": "rm -rf dist",
    "set-config": "chmod +x scripts/set-config.sh && ./scripts/set-config.sh",
    "build:dev": "pnpm run clean && pnpm run set-config development && tsc",
    "build:prod": "pnpm run clean && pnpm run set-config production && tsc",
    "publish": "pnpm publish",
    "test-linter": "eslint . --ext .ts",
    "test-type": "tsc --noEmit",
    "link": "pnpm link --global",
    "unlink": "pnpm unlink",
    "watch": "tsc --watch",
    "build:bundle:esm": "pnpm run build:prod && mkdir -p bundle && esbuild src/index.ts --bundle --platform=browser --format=iife --global-name=DialogSDK --minify --outfile=bundle/dialog-sdk.$npm_package_version.min.js",
    "deploy:s3": "aws s3 cp bundle/dialog-sdk.$npm_package_version.min.js s3://dialog-sdk/",
    "deploy:cloudfront": "aws cloudfront create-invalidation --distribution-id E3NRPUUF94K3P6 --no-cli-pager --paths \"/dialog-sdk.$npm_package_version.min.js\"",
    "deploy:cdn": "pnpm run build:prod && pnpm run build:bundle:esm && pnpm run deploy:s3 && pnpm run deploy:cloudfront",
    "deploy:cdn:publish": "pnpm run build:prod && pnpm run build:bundle:esm && pnpm run deploy:s3 && pnpm run deploy:cloudfront && pnpm publish"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "dialog",
    "ai",
    "assistant",
    "ecommerce",
    "sdk",
    "typescript"
  ],
  "author": "Dialog",
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "esbuild": "^0.25.8",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-import-resolver-typescript": "^4.3.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^5.2.6",
    "typescript": "^5.8.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "posthog-js": "^1.235.3",
    "uuidv7": "^1.0.2"
  },
  "packageManager": "pnpm@9.15.1+sha512.1acb565e6193efbebda772702950469150cf12bcc764262e7587e71d19dc98a423dff9536e57ea44c49bdf790ff694e83c27be5faa23d67e0c033b583be4bfcf"
}
